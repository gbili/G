SELECT vt.value AS title, CONCAT(sh.host, '', s.path) AS hostUrl, sh.hFName AS hostName, i.localUrl AS imageLocalUrl, vc.value AS catName, vc.slug AS catSlug, v.timeLength AS timeLength, v.date AS dateTimestamp FROM G_Vid AS v  INNER JOIN G_Vid_Title AS vt ON (v.titleId = vt.elementId) INNER JOIN G_Source AS s ON (v.sourceId = s.sourceId) INNER JOIN G_Source_Host AS sh ON (s.hostId = sh.hostId) INNER JOIN G_Vid_Image AS i ON (v.imageId = i.imageId) INNER JOIN G_Vid_Category AS vc ON (v.categoryId = vc.elementId)